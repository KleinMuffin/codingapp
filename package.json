{
  "name": "codingapp",
  "version": "0.3.0",
  "description": "condingapp",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "releaseml": "electron-builder -ml --publish onTagOrDraft",
    "releasew": "electron-builder -w --publish onTagOrDraft"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonmeusel/codingapp.git"
  },
  "build": {
    "appId": "de.simonmeusel.codingapp",
    "files": [
      "src/**/*"
    ],
    "publish": [
      "github"
    ]
  },
  "author": "Simon Meusel",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "cheerio": "^1.0.0-rc.2",
    "electron-settings": "^3.1.4"
  },
  "devDependencies": {
    "electron": "^1.8.1",
    "electron-builder": "^19.55.1"
  }
}
